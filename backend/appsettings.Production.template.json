{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:3001"
      }
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "server=localhost;user=YOUR_USERNAME;password=YOUR_PASSWORD;database=deelfabriekDB;"
  },
  "Smtp": {
    "Host": "YOUR_SMTP_HOST",
    "Port": 587,
    "Username": "YOUR_SMTP_USERNAME",
    "Password": "YOUR_SMTP_PASSWORD",
    "FromEmail": "YOUR_FROM_EMAIL",
    "FromName": "YOUR_FROM_NAME",
    "EnableSsl": true
  }
}
